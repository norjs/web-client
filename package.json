{
  "name": "norjs",
  "version": "1.3.2",
  "description": "The NorJS Runtime Environment",
  "main": "client/app/index.js",
  "directories": {
    "doc": "docs"
  },
  "bin": {
    "norjs": "./bin/norjs.sh"
  },
  "scripts": {
    "compile": "babel src/bin --out-dir dist --presets=es2015",
    "compile-watch": "babel --watch src --out-dir dist --presets=es2015",
    "pretest": "npm run -s compile",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "npm run -s compile",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/norjs/norjs.git"
  },
  "keywords": [
    "angularjs",
    "webpack",
    "es6",
    "karma",
    "javascript",
    "ecmascript",
    "nodejs",
    "babel",
    "runtime",
    "runtime-environment"
  ],
  "author": "Jaakko-Heikki Heusala <jheusala@iki.fi>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/norjs/norjs/issues"
  },
  "homepage": "https://github.com/norjs/norjs#readme",
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-es2015": "^6.24.1",
    "mocha": "^5.2.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "lodash": "^4.17.11",
    "minimist": "^1.2.0",
    "nor-exec": "^1.0.1"
  }
}
